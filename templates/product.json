    "image_with_text_HDYRWd": {
      "type": "image-with-text",
      "blocks": {
        "heading_h4rkf6": {
          "type": "heading",
          "settings": {
            "heading": "Kiln Dried Wood is a brilliant fuel source",
            "heading_size": "h2"
          }
        },
        "text_F9xAjB": {
          "type": "text",
          "settings": {
            "text": "<p>Kiln Dried Wood generate beautiful flames and provide excellent heat. These logs contain a low moisture content which is better for the environment and those near the fire. </p><p>For more information on kiln-dried wood, explore the our website.</p>",
            "text_style": "body"
          }
        }
      },
      "block_order": ["heading_h4rkf6", "text_F9xAjB"],
      "settings": {
        "image": "shopify://shop_images/Rectangle_16.jpg",
        "height": "adapt",
        "desktop_image_width": "medium",
        "layout": "image_first",
        "desktop_content_position": "middle",
        "desktop_content_alignment": "left",
        "content_layout": "no-overlap",
